@using TravelTrip.Models.Sınıflar
@model List<Blog>

<div class="box1-left">
    @foreach (var y in Model)
    {

        <div class="bot-img">
            <img src="@y.BlogImage" alt="" />
        </div>
        <div class="bot-desc1">
            <p>@y.Aciklama.Substring(0, 75)...</p>
        </div>
        <div class="clear"></div>
    }


</div>